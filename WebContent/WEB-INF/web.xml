<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>hello-basic</display-name>
	<description></description>

	
	<servlet>
		<servlet-name>TaskServlet</servlet-name>
		<servlet-class>example.TaskServlet</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>TaskServlet</servlet-name>
		<url-pattern>/task</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UserTaskServlet</servlet-name>
		<servlet-class>example.UserTaskServlet</servlet-class>
	</servlet>

	
	<servlet-mapping>
		<servlet-name>UserTaskServlet</servlet-name>
		<url-pattern>/usertask</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
   <servlet-name>HelloWorld</servlet-name>
   <servlet-class>example.HelloWorld</servlet-class>
   </servlet>

    <servlet-mapping>
   <servlet-name>HelloWorld</servlet-name>
   <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
    

	<!-- Welcome files -->
	<welcome-file-list>
		
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	
</web-app>